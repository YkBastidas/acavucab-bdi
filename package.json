    "express": "^4.16.4",
    "helmet": "^3.21.1",
    "http-errors": "^1.7.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.6.1",
    "universal-cookie-express": "^3.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
